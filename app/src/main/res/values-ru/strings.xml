    <string name="help">Помощь</string>
    <string name="help_device_info">Информация об устройстве</string>
    <string name="help_device_info_description">Модель · CSC</string>
    <string name="help_device_info_edit">Редактировать</string>
    <string name="help_device_name">Название устройства</string>
