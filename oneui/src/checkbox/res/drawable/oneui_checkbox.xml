<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:drawable="@drawable/sesl_btn_check_to_on_025"
        android:state_checked="true"
        android:state_enabled="false" />
    <item
        android:drawable="@drawable/sesl_btn_check_to_on_001"
        android:state_enabled="false" />
    <item
        android:id="@id/checked"
        android:drawable="@drawable/sesl_btn_check_to_on_026"
        android:state_checked="true" />
    <item
        android:id="@id/unchecked"
        android:drawable="@drawable/sesl_btn_check_to_on_000"
        android:state_checked="false" />
    <transition
        android:drawable="@drawable/sesl_btn_check_to_on_024"
        android:fromId="@id/unchecked"
        android:toId="@id/checked" />
    <transition
        android:drawable="@drawable/sesl_btn_check_to_on_002"
        android:fromId="@id/checked"
        android:toId="@id/unchecked" />
</animated-selector>